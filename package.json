{
  "name": "ipr",
  "version": "0.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "prettier": "prettier --write {e2e,src}/**/*.{js,ts,css,scss,html}",
    "stylelint": "stylelint src/**/*.{css,scss}",
    "stylelint-fix": "stylelint --fix src/**/*.{css,scss}",
    "dev:ssr": "ng run ipr:serve-ssr",
    "serve:ssr": "node dist/ipr/server/main.js",
    "build:ssr": "ng build --configuration production && ng run ipr:server:production",
    "prerender": "ng run ipr:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.1",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "^12.2.1",
    "@angular/compiler": "^12.2.1",
    "@angular/core": "^12.2.1",
    "@angular/forms": "^12.2.1",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "^12.2.1",
    "@angular/platform-browser-dynamic": "^12.2.1",
    "@angular/platform-server": "~12.2.1",
    "@angular/router": "^12.2.1",
    "@apollo/client": "^3.4.7",
    "@nguniversal/express-engine": "^12.1.0",
    "@ngxs/store": "^3.7.2",
    "apollo-angular": "^2.6.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.1",
    "@angular/cli": "^12.2.1",
    "@angular/compiler-cli": "^12.2.1",
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-apollo-angular": "3.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.8.2",
    "@types/node": "^12.20.19",
    "codelyzer": "^6.0.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-prettier": "^1.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,css,scss,md,html}": "prettier --write",
    "**/*.{css,scss}": [
      "stylelint --fix",
      "stylelint"
    ]
  }
}
